@model SaniShop.Models.SalesDetailModel

@{
    ViewBag.Title = "SalesIndex";
}

<h2>Sales Home</h2>
@using (Html.BeginForm("Index", "Sales"))
{

    <html>

    <body>
        <div>
            <table>
                <tr>
                    <td>id </td>
                    <td>
                        @Html.TextBoxFor(m => m.id, new { @class = "inputComn", @placeholder = "City" })
                    </td>
                </tr>
                <tr>
                    <td>Iteam </td>
                    <td>
                        @Html.TextBoxFor(m => m.Product_id, new { @class = "inputComn", @placeholder = "City" })
                    </td>
                </tr>
                <tr>
                    <td>Price </td>
                    <td>
                        @Html.TextBoxFor(m => m.Product_price, new { @class = "inputComn", @placeholder = "City" })
                    </td>
                </tr>
                <tr>
                    <td>customer </td>
                    <td>
                        
                        @Html.TextBoxFor(m => m.Quantity, new { @class = "inputComn", @placeholder = "City" })
                    </td>
                </tr>

            </table>
        </div>
        <div>
            <input type="submit" class="contactBtn signupBtn" value="Submit" />
        </div>
    </body>
</html>
}